;
; /* Start:"a:4:{s:4:"full";s:58:"/local/templates/avantage/assets/js/app.js?149967453912336";s:6:"source";s:42:"/local/templates/avantage/assets/js/app.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
/**
 * Created by kirshin on 19.03.15.
 */
var app = function () {

    var js = function() {
        $("html").removeClass("no-js");
    }

    var popup = function () {
        $('.image-popup').magnificPopup({type: 'image'});

        $('.gallery-popup').each(function () { // the containers for all your galleries
            $(this).magnificPopup({
                delegate: 'a.photo', // the selector for gallery item
                type: 'image',
                image: {
                    markup: '<div class="mfp-figure">' +
                    '<div class="mfp-close"></div>' +
                    '<div class="mfp-img-bg"><div class="mfp-img"></div></div>' +
                    '<div class="mfp-bottom-bar">' +
                    '<div class="mfp-title"></div>' +
                    '<div class="mfp-counter"></div>' +
                    '</div>' +
                    '</div>'
                },
                gallery: {
                    enabled: true,
                    tPrev: 'Предыдущая', // title for left button
                    tNext: 'Следующая', // title for right button
                    tCounter: '<span class="mfp-counter">Фотография %curr% из %total%</span>' // markup of counter
                },
                callbacks: {
                    elementParse: function (item) {
                        // Function will fire for each target element
                        // "item.el" is a target DOM element (if present)
                        // "item.src" is a source that you may modify

                        //console.log(item); // Do whatever you want with "item" object
                    },
                    change: function() {
                        //console.log('Content changed');
                        //console.log(this.content); // Direct reference to your popup element
                        var img  = this.content.find("img"),
                            link = img.attr("src"),
                            h = $(window).height()-80;
                        img.css({"max-height":h});
                        this.content.find(".mfp-img-bg").trigger('zoom.destroy');
                        this.content.find(".mfp-img-bg").zoom({
                            url: link,
                            on: 'click',
                            magnify: 1.5
                        });
                    },
                    imageLoadComplete: function() {
                        // fires when image in current popup finished loading
                        // avaiable since v0.9.0
                        //console.log('Image loaded');
                        var h = $(window).height()-80;
                        console.log(h);
                        $(".mfp-img-bg img").css({"max-height":h});
                        /*
                        $(".mfp-wrap").on("click",".zoomImg",function(){
                            $(".mfp-arrow-right").trigger("click");
                        })
                        */
                    }
                }
            });
        });

        $('.popup').magnificPopup({type: 'inline'});

        $('.ajax-popup').click(function(e){
            e.preventDefault();
            var url = $(this).attr('href');
            url += (url.indexOf('?') > -1 ? '&' : '?') + 'popup=Y';
            $.magnificPopup.open({
                items: {
                    type: 'ajax',
                    src: url
                },
                callbacks: {
                    ajaxContentAdded: function () {
                        mask();
                    }
                }
            });
        });

        $('.popup-youtube, .popup-vimeo, .popup-gmaps').magnificPopup({
            disableOn: 700,
            type: 'iframe',
            mainClass: 'mfp-fade',
            removalDelay: 160,
            preloader: false,

            fixedContentPos: false
        });

        $(document).on('click', '.popup-modal-close', function (e) {
            e.preventDefault();
            $.magnificPopup.close();
        });
    };


    var favorites = function () {
        $('#add-to-favorite').click(function () {
            $.ajax({
                url: '/local/ajax/favorite.php',
                data: 'action=add&id=' + $(this).data('rel')
            }).done(function (response) {
                $('#top-favorite').text(response);
                if (parseInt(response)) {
                    $('#top-favorite').parent().show();
                } else {
                    $('#top-favorite').parent().hide();
                }
            });
            $('.in-favorites').show();
            $(this).parent().hide();
            $.magnificPopup.open({

                items: {
                    src: '#add-to-favorite-popup',
                    type: 'inline'
                }
            });

            return false;
        });
        $('.remove-from-favorite').click(function () {
            $.ajax({
                url: '/local/ajax/favorite.php',
                data: 'action=remove&id=' + $(this).data('rel')
            }).done(function (response) {
                $('#top-favorite').text(response);
                if (parseInt(response)) {
                    $('#top-favorite').parent().show();
                } else {
                    $('#top-favorite').parent().hide();
                }
                location.reload(true);
            });
            //$(this).parent().parent().remove();

            return false;
        });
    };

    var ajaxPaging = function () {
        $('#ajax-paging').click(function () {
            var link = $(this);
            var href = $(this).attr('href');
            $.ajax({
                url: href
            }).done(function (response) {
                link.parent().before(response);
                link.parent().remove();
                window.history.pushState({path: href}, '', href);
                ajaxPaging();
            });
            return false;
        });
    };

    var mask = function () {
        $("input.telephone").mask("+7 (999) 999-99-99");
    };

    var hideBanner = function () {
        $(".banner-hide").click(function () {
            $(this).parent().parent().hide();
            $.ajax({url: "/local/ajax/hide.banner.php"});
            return false;
        });
    };

    var styler = function(){
        $("select.style").styler();
    };

    var callbackTop = function() {
        var block = $("#callback");
        if ($(window).scrollTop() < 85 ) {
            block.css({"top": 85-$(window).scrollTop()})
        } else {
            block.css({"top":0})
        }
    };

    var setCallback = function() {
        var block = $("#callback");
        block.find("div.btn").click(function () {
            var form = block.find("form");
            if (parseFloat(form.css("bottom")) === 0) {
                form.animate({"bottom": - form.outerHeight() + "px"}, 200);
            } else {
                form.animate({"bottom": 0}, 200);
            }
        });
        callbackTop();
        $(window).scroll(function () {
            callbackTop();
        });
    };

    var goDown = function() {
        $(".go-down").click(function(){
            var lnk = $($(this).closest(".c-block").next()).offset().top,
                hmtlnow = $("html, body");
            hmtlnow.animate({ scrollTop: lnk}, 1100 );
      });
    };

    var picZoom = function() {
        $(".big-picture a").each(function() {
            var link = $(this).attr("href");
            $(this).zoom({
                url: link
            });
        });
        $(".more-pics .item a").click(function(){
            var item = $(this).parent(),
                num = item.index(),
                pic = $(".big-picture");
            $(".more-pics .item").removeClass("active");
            item.addClass("active");
            pic.hide();
            pic.eq(num).show();
            return false;
        });
    };

    var rightBlock = function() {
        var panel = 0;
        if ( $("#bx-panel").length ) {
            panel = $("#bx-panel").height();
            //console.log(panel)
        }
        $(".left-banners").css({"top":(150-$(window).scrollTop() + panel )});
        $(window).scroll(function () {
            if ($(this).scrollTop() < (80)) {
                //console.log($(this).scrollTop());
                $(".left-banners").css({"top":(150+panel-$(this).scrollTop())});
            } else {
                $(".left-banners").css({"top":68+panel});
            }
        });
    };

    var adaptiveMenu = function() {
        var $html = $('html'),
            $hamburger = $('#hamburger'),
            $menuOriginal = $('#my-menu'),
            $catalog = $menuOriginal.find(".left-menu"),
            $ul="";
        $catalog.each(function(){
           $ul = $ul+$(this).children("ul").html();
        });

        $menuOriginal.before("<div id='mobile-menu'><ul>"+$ul+"</ul></div>");

        $("#mobile-menu").find("ul").show();

        var $menu = $('#mobile-menu').mmenu({
            "extensions": [
                "effect-menu-zoom",
                "effect-panels-zoom",
                "pagedim-black",
                "theme-white"
            ],
            counters: false,
            dividers: {
                fixed: false
            },
            navbar: {
                title: 'Меню'
            },
            navbars: [
                {
                    //position: 'top',
                    content: '<div class="mm-logo"><a href="/" title="перейти на главную"><img src="/local/templates/avantage/assets/images/logo2.png" width="250" height="48" alt=""/></a></div>'
                }, {
                    position: 'top',
                    content: [
                        'prev',
                        'title',
                        'close'
                    ],
                    classNames: {
                        selected: "active"
                    }
                }, {
                    position: 'bottom',
                    content: [
                        ''//'<a href="/"><span class="icon-home"></span> перейти на главную</a>'
                    ]
                }
            ],
            "iconPanels": true
        }).data('mmenu');

        $hamburger.on('click', function (e) {
            e.preventDefault();
            $html.hasClass('mm-opened') ? $menu.close()  : $menu.open()
        });

        $menu.bind('close:finish', function () {
            setTimeout(function () {
                $hamburger.children('.hamburger').removeClass('is-active')
            }, 100)
        });

        $menu.bind('open:finish', function () {
            setTimeout(function () {
                $hamburger.children('.hamburger').addClass('is-active')
            }, 100)
        });

    };

    var owl = function() {

        var $owl = $(".title-slider");
        $owl.each(function(e){
            var $this = $(this),
                sliderOptions = $this.data('owl-options'),
                slides = $this.find(".item").length,
                sliderDefaults = {
                    loop: slides,
                    margin: 0,
                    dots: slides > 1,
                    nav: slides > 1,
                    navText: ["",""],
                    autoplay: false,
                    items: 1
                };
            $this.owlCarousel( $.extend( sliderDefaults, sliderOptions) );
        });

    };

    return {
        init: function () {
            js();
            mask();
            popup();
            favorites();
            ajaxPaging();
            hideBanner();
            styler();
            setCallback();
            goDown();
            picZoom();
            rightBlock();
            adaptiveMenu();
            owl();
        }
    }
}();

$(document).ready(function () {
    app.init();
});
/* End */

function window_height() {
	$(".c-block").css({"min-height":0})
	var w_height = $(window).height()
	$(".c-block").css({"min-height":w_height});
   // $(".c-block.back1").css({"margin-top":85})
	var minHeight = Math.max(364, w_height-240);
	$(".vert").css({"height":minHeight})
}
function cat_height() {
	$(".catalog-block").each(function(){
		var h = $(this).find(".catalog-list .catalog-item .picture").height();
		$(this).find(".name-block span").css({"height":h})
	});
}

